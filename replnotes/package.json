{
  "name": "replnotes",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --dotenv .env",
    "firebase": "npm run default && firebase emulators:start --import tests/.seed",
    "export": "firebase emulators:export tests/.seed",
    "default": "firebase use default",
    "prod": "firebase use prod",
    "build": "rm -rf .nuxt && nuxt build --dotenv .env.staging.local",
    "build:prod": "rm -rf .nuxt && nuxt build --dotenv .env.production.local",
    "start": "nuxt start --dotenv .env.staging.local",
    "generate": "nuxt generate --dotenv .env.staging.local",
    "deploy": "npm run default && firebase deploy",
    "deploy:prod": "npm run prod && firebase deploy",
    "test": "firebase emulators:exec 'jest --env=node'",
    "reinstall": "rm package-lock.json && rm -rf node_modules && npm i"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/firebase": "^7.5.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sitemap": "^2.4.0",
    "axios": "^0.21.1",
    "core-js": "^3.11.1",
    "firebase": "^8.2.4",
    "firebase-admin": "^9.2.0",
    "highlight.js": "^10.7.2",
    "ipynb2html": "^0.3.0",
    "nodom": "^2.4.0",
    "nuxt": "^2.15.3"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^4.0.1",
    "@tailwindcss/typography": "^0.4.0",
    "@firebase/rules-unit-testing": "1.2.2",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "postcss": "^8.2.13",
    "vue-jest": "^3.0.4"
  }
}
