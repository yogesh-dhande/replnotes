{
  "name": "user",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "PORT=8081 nuxt",
    "dev:static": "DOMAIN=yogesh.replnotes.com PORT=8081 nuxt",
    "default": "firebase use staging && gcloud config set project staging-2cacb",
    "prod": "firebase use production && gcloud config set project nbtoblog-8a03f",
    "build": "rm -rf .nuxt && nuxt build",
    "generate": "USERNAME=yogesh DOMAIN=yogesh.replnotes.com nuxt generate",
    "start": "nuxt start",
    "deploy:hosting": "npm run default && firebase deploy --only hosting:user-replnotes",
    "deploy:hosting:prod": "npm run prod && firebase deploy --only hosting:user-replnotes",
    "deploy": "npm run default && gcloud builds submit --substitutions=_DEPLOY_TARGET='staging'",
    "deploy:prod": "npm run prod && gcloud builds submit --substitutions=_DEPLOY_TARGET='production'",
    "reinstall": "rm package-lock.json && rm -rf node_modules && npm i",
    "test": "firebase emulators:exec 'jest --env=node'",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/firebase": "^7.5.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sitemap": "^2.4.0",
    "@splitbee/web": "^0.2.4",
    "aos": "^2.3.4",
    "axios": "^0.21.1",
    "core-js": "^3.11.1",
    "firebase": "^8.2.4",
    "firebase-admin": "^9.2.0",
    "highlight.js": "^10.7.2",
    "ipynb2html": "^0.3.0",
    "nodom": "^2.4.0",
    "nuxt": "^2.15.3",
    "postscribe": "^2.0.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@firebase/rules-unit-testing": "1.2.2",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.0.1",
    "@tailwindcss/typography": "^0.4.0",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "jest": "^26.6.3",
    "postcss": "^8.2.13",
    "vue-jest": "^3.0.4",
    "watch": "^1.0.2"
  }
}